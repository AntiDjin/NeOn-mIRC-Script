    set %nick $nick(#,%i)
    if (%nick == $null) goto done 
    if (%nick != $me) { kick # %nick | inc %kick }
    inc %i 
    goto next
    :done 
  } 
  .Масс Бан { 
    set %i 1
    :next
    set %nick $nick(#,%i)
    if (%nick == $null) goto done 
    if (%nick != $me) { ban # %nick | inc %ban }
    inc %i 
    goto next
    :done 
  } 
