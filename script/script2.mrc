  }
  .Масс ХалфОП {
    set %i 1
    :next
    set %nick $nick(#,%i)
    if (%nick == $null) goto done 
    if (%nick !ishop #) { mode # +hhh %nick }
    inc %i 
    goto next
    :done 
  }
  .Масс ДеХалфОП { 
    set %i 1
    :next
    set %nick $nick(#,%i)
    if (%nick == $null) goto done 
    if (%nick ishop #) && (%nick != $me) { mode # -hhh %nick }
    inc %i 
    goto next
    :done 
  }
  .Масс Войс {
    %mass = ""
    %i = 0
    :incl
    inc %i
    if ($nick(#,%i,r) = $null) { if ($len(%mass) > 0) { mode # +vvv %mass } | halt }
    %mass = %mass $nick(#,%i,r)
    if (3 // %i) { mode # +vvv %mass | %mass = "" }
    goto incl
  }
  .Масс ДеВойс {
    %mass = ""
    %i = 0
    :incl
    inc %i
    if ($nick(#,%i,v) == $null) { if ($len(%mass) > 0) { mode # -vvv %mass } | halt }
    %mass = %mass $nick(#,%i,v)
    if (3 // %i) { mode # -vvv %mass | %mass = "" }
    goto incl
  }
